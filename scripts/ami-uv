#!/usr/bin/env bash
# ami-uv: Universal UV execution wrapper for AMI Orchestrator
# Use this instead of direct uv commands
#
# DEPRECATED: Use ami-run uv [args...] instead
# This wrapper is maintained for backward compatibility
set -euo pipefail

# Forward to the new unified ami-run command
exec "$(dirname "$(dirname "${BASH_SOURCE[0]}")")/scripts/ami-run" uv "$@"
